{
	"name": "@sixa/faq",
	"version": "1.0.0",
	"description": "FAQ block for WordPress editor.",
	"keywords": [
		"questions",
		"accordion"
	],
	"homepage": "https://github.com/sixach/sixa-wp-block-faq#readme",
	"bugs": {
		"url": "https://github.com/sixach/sixa-wp-block-faq/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/sixa-wp-block-faq.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"main": "build/index.js",
	"scripts": {
		"dev": "wp-scripts start",
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css-fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.css": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format:js"
		]
	},
	"dependencies": {
		"@wordpress/api-fetch": "^3.23.1",
		"@wordpress/block-editor": "^5.3.3",
		"@wordpress/blocks": "^8.0.3",
		"@wordpress/components": "^13.0.3",
		"@wordpress/data": "^4.27.3",
		"@wordpress/element": "^2.20.3",
		"@wordpress/hooks": "^2.12.3",
		"@wordpress/i18n": "^3.20.0",
		"@wordpress/url": "^2.22.2"
	},
	"devDependencies": {
		"@sixa/wp-block-utils": "^0.10.0",
		"@wordpress/scripts": "^15.0.1",
		"autoprefixer": "^9.8.6",
		"classnames": "^2.3.1",
		"husky": "^6.0.0",
		"lint-staged": "^10.5.4",
		"lodash": "^4.17.21",
		"postcss-loader": "^3.0.0",
		"postcss-nested": "^4.2.1",
		"postcss-nested-ancestors": "^2.0.0",
		"postcss-prefixer-keyframes": "0.0.3",
		"postcss-selector-replace": "^1.0.2",
		"rename-keys": "^2.0.1"
	}
}
